{
  "name": "rnstoragemanager",
  "displayName": "RNStorageManager",
  "description": "Extensión para la gestión del Storage en aplicaciones de React Native",
  "version": "0.0.3",
  "publisher": "rogercg",
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Debuggers",
    "Other"
  ],
  "main": "./extension.js",
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#C80000",
    "theme": "dark"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.viewStorage",
        "title": "View React Native Storage",
        "category": "React Native"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.97.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.19.0",
    "@vscode/vsce": "^2.24.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rogercg/rn-storage-manager"
  },
  "dependencies": {
    "ws": "^8.18.0"
  }
}
